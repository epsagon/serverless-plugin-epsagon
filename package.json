{
  "name": "serverless-plugin-epsagon",
  "version": "0.0.0-development",
  "description": "Epsagon's plugin for the serverless framework",
  "main": "dist/bundle.js",
  "scripts": {
    "lint:js": "eslint --max-warnings=0 ./src/ -f table --ext .js --ext .jsx",
    "lint": "npm run lint:js",
    "build:dev": "rollup -c",
    "build": "NODE_ENV=production rollup -c",
    "clean": "rm -r dist/",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epsagon/serverless-plugin-epsagon.git"
  },
  "keywords": [
    "serverless",
    "lambda",
    "epsagon",
    "tracing",
    "distributed-tracing",
    "aws-lambda",
    "debugging",
    "monitoring"
  ],
  "author": "Epsagon Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epsagon/serverless-plugin-espagon/issues"
  },
  "homepage": "https://github.com/epsagon/serverless-plugin-espagon#readme",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "just-extend": ">=4.0.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-uglify-es": "0.0.1",
    "semantic-release": "^17.0.1",
    "sinon": "^4.3.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "lodash": "^4.17.11",
    "util.promisify": "^1.0.0"
  }
}
